{
	"manifest_version": 2,

	"name": "TabKiller",
	"description": "Opera extension to close tab duplicates and search tabs.",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
	"permissions": ["tabs", "contextMenus"],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "TabKiller"
	}
}